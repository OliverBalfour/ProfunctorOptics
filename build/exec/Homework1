#!/bin/sh
# @generated by Idris 0.4.0-aa77b7f33, Chez backend

set -e # exit on any error

if [ "$(uname)" = Darwin ]; then
  DIR=$(zsh -c 'printf %s "$0:A:h"' "$0")
else
  DIR=$(dirname "$(readlink -f -- "$0")")
fi

export LD_LIBRARY_PATH="$DIR/Homework1_app:$LD_LIBRARY_PATH"
export IDRIS2_INC_SRC="$DIR/Homework1_app"
"$DIR/Homework1_app/Homework1.so" "$@"
